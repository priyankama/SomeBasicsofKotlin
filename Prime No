fun main(args: Array<String>) {
    println("Hello, world!")
    val n = 2110
    var f=2
    if(n==1){
        print("Co-prime no..")
    }
    else{
        for(i in n-1 downTo 2){
            if(n%i==0){
                f=1
                break
            }
        }
    if(f==1){
        print("Composite no")
    }else{
        print("Prime no")
    }
    }
    
}
